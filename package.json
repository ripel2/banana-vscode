{
  "name": "banana-vscode",
  "displayName": "banana-vscode",
  "description": "Epitech coding style verification for VSCode",
  "publisher": "ripel2",
  "repository": {
    "type": "git",
    "url": "https://github.com/ripel2/banana-vscode"
  },
  "version": "1.1.0",
  "engines": {
    "vscode": "^1.72.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./src/extension.js",
  "contributes": {
    "commands": [{
      "command": "banana-vscode.startBanana",
      "title": "Banana: Check for any coding style errors in the current working directory"
    },
    {
      "command": "banana-vscode.clearAllBananaErrors",
      "title": "Banana: Clear all the detected coding style errors"
    }]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.72.0",
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.0",
    "@types/node": "16.x",
    "eslint": "^8.24.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.8.4",
    "@vscode/test-electron": "^2.1.5"
  }
}
